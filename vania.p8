pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- peacefulvania v0.1
-- <me@lmorchard.com>

debug = false

world = {
 gravity=0.25
}

function _init()
 init_player()
end

function _update()
	update_player()
end

function _draw()
 if debug then fade_cls() else cls() end
 map_tiles.draw()
 draw_player()
end

function fade_cls()
 for x=0,128 do
  for y=0,128 do
   if rnd(100) < 30 then
    pset(x, y, 0)
   end
  end
 end
end

function init_player()
 player = {
  -- borrowed from http://pixeljoint.com/pixelart/77341.htm
  sprite=sprites.init({0,1,2,3,4,5,6,7}, 0.3, 1, 1),
  x=10, y=10,
  w=7, h=7,
  dx=0, dy=0,
  gravity=true,
  inertia=0.6,
  run=0.7,
  jump=-2.6,
  djump={
   available=true,
   ready=true,
   active=false
  }
 }
end

function update_player()
 if btn(0) then  -- left
  player.sprite.flipx = true
  player.sprite.animating = true
  player.dx = -player.run
 elseif btn(1) then  -- right
  player.sprite.flipx = false
  player.sprite.animating = true
  player.dx = player.run
 else
  player.dx = 0 -- todo need momentum decay
  player.sprite.animating = false
 end
 
 if btnp(4) then  -- jump
  if is_solid_below(player) then
   player.dy = player.jump
   if player.djump.available then
    player.djump.ready = true
    player.djump.active = false
   end
  elseif player.djump.available and player.djump.ready then
   player.dy = player.jump
   player.djump.ready = false
   player.djump.active = true
  end
 end

 if player.dy >= 0 then
  player.djump.active = false
 end

 motion.update(player)
end

function draw_player()
 if player.djump.active then
  -- todo jetpack particles?
  pal(1, 12)
 end
 sprites.draw(player)
 pal()
end

function is_solid_below(o)
 return
  map_tiles.is_solid_at(o.x,o.y+o.h+1) or 
  map_tiles.is_solid_at(o.x+o.w, o.y+o.h+1)
end

function is_solid_intersect(x, y, w, h)
 return
  map_tiles.is_solid_at(x, y) or
  map_tiles.is_solid_at(x+w, y) or
  map_tiles.is_solid_at(x+w, y+h/2) or
  map_tiles.is_solid_at(x, y+h/2) or
  map_tiles.is_solid_at(x+w, y+h) or
  map_tiles.is_solid_at(x, y+h)
end

map_tiles = {
 flags={
  solid_top=0,
  solid_bottom=1,
  slope_right=2,
  slope_left=3
 }
}

function map_tiles.draw()
 map(0, 0, 0, 0, 16, 16)
end

function map_tiles.is_solid_at(x, y)
 if x < 0 then return true end
 if y < 0 then return true end

 local map_x = flr(x / 8)
 local map_y = flr(y / 8)
 local tile_x = flr(x % 8)
 local tile_y = flr(y % 8)
 local tile = mget(map_x, map_y)

 if debug then pset(x, y, 8) end

 if fget(tile, map_tiles.flags.slope_left) then
  return tile_y >= tile_x
 elseif fget(tile, map_tiles.flags.slope_right) then
  return tile_y >= (8 - tile_x)
 end
 if tile_y < 4 then
  return fget(tile, map_tiles.flags.solid_top)
 else
  return fget(tile, map_tiles.flags.solid_bottom)
 end
end

motion = {}

function motion.update(o)
 if o.gravity then
  motion.apply_gravity(o)
 end

 if not is_solid_intersect(o.x + o.dx, o.y, o.w, o.h) then
  o.x += o.dx
 elseif o.dx != 0 then
  if fget(mget((o.x + o.dx) / 8, (o.y + o.h) / 8), map_tiles.flags.slope_left) or
     fget(mget((o.x + o.w - 1 + o.dx) / 8, (o.y + o.h) / 8), map_tiles.flags.slope_right) then
   o.x += o.dx
   o.y -= 1
   pset(o.x, o.y, 14)
  end
 end

 if not is_solid_intersect(o.x, o.y + o.dy, o.w, o.h) then
  o.y += o.dy
 end
 -- o.dx *= o.inertia
 -- o.dy *= o.inertia
end

function motion.apply_gravity(o)
 local ndy = o.dy + world.gravity
 local on_ground = 
  map_tiles.is_solid_at(o.x, o.y + o.h + ndy) or 
  map_tiles.is_solid_at(o.x + o.w, o.y + o.h + ndy)
 o.dy = on_ground and 0 or ndy
end

sprites = {}

function sprites.init(frames, speed, sw, sh)
 return {
  frames=frames,
  sw=sw, sh=sh,
  speed=speed,
  animating=true,
  index=1,
  flipx=false,
  flipy=false
 }
end

function sprites.draw(o)
 local s = o.sprite
 local f
 if not s.animating then
  f = s.frames[1]
 else
  s.index += s.speed
  if s.index > count(s.frames)+1 then
   s.index = 1
  end
  f = s.frames[flr(s.index)]
 end
 spr(f, o.x, o.y, s.sw, s.sh, s.flipx, s.flipy)
end

